  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/railroadpm/site/issues"
  },
  "homepage": "https://github.com/railroadpm/site#readme",
  "devDependencies": {
    "rimraf": "^2.6.2"
  }
}
