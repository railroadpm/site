{
  "files.insertFinalNewline": true,
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "bin": true,
    "node_modules": false,
    "dist": false,
    "**/*.min.css": true
  },
  "files.associations": {
    "api/layouts/**/*.html": "gohtml",
    "api/layouts/**/*.json": "gotemplate",
    "admin/*.html": "html"
  },
  "autorun.build": false,
  "html.format.indentInnerHtml": false,
  "vetur.validation.template": false,
  "vetur.format.defaultFormatter.html": "js-beautify-html",
  "vetur.format.defaultFormatterOptions": {
    "js-beautify-html": {
      "wrap_line_length": 120,
      "wrap_attributes": "auto",
      "content_unformatted": ["vue-markdown"]
    }
  },
  "prettier.disableLanguages": ["js", "vue"],
  "prettier.eslintIntegration": false,
  "prettier.singleQuote": true,
  "prettier.printWidth": 170,
  "eslint.run": "onSave",
  "eslint.alwaysShowStatus": true,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [{
      "language": "vue",
      "autoFix": true
    },
    {
      "language": "html",
      "autoFix": true
    },
    {
      "language": "javascript",
      "autoFix": true
    }
  ],
  "eslint.options": {
    "root": true,
    "env": {
      "browser": true,
      "node": true
    },
    "plugins": ["vue", "prettier"],
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaVersion": 2017,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      }
    },
    "extends": ["plugin:vue/essential", "prettier"],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 170
        }
      ],
      "max-len": [
        "warn",
        {
          "code": 170
        }
      ],
      "object-curly-spacing": "off",
      "object-property-newline": [
        "error",
        {
          "allowMultiplePropertiesPerLine": true
        }
      ],
      "object-curly-newline": [
        "error",
        {
          "ObjectExpression": {
            "multiline": false,
            "consistent": true
          },
          "ObjectPattern": {
            "multiline": false,
            "consistent": true
          }
        }
      ],
      "comma-dangle": "off",
      "quotes": [2, "single"]
    }
  },
  "stylusSupremacy.insertColons": true,
  "stylusSupremacy.insertNewLineBetweenSelectors": true,
  "stylusSupremacy.insertSemicolons": false,
  "stylusSupremacy.insertBraces": false,
  "stylusSupremacy.insertSpaceAfterComment": true,
  "stylusSupremacy.insertSpaceBeforeComment": true,
  "stylusSupremacy.sortProperties": "grouped",
  "stylusSupremacy.insertParenthesisAroundIfCondition": true,
  "todo-tree.flat": true,
  "todo-tree.grouped": true,
  "todo-tree.expanded": true,
  "cSpell.enabledLanguageIds": [
    "c",
    "cpp",
    "csharp",
    "go",
    "handlebars",
    "javascript",
    "javascriptreact",
    "json",
    "latex",
    "markdown",
    "php",
    "plaintext",
    "python",
    "restructuredtext",
    "text",
    "typescript",
    "typescriptreact",
    "yml"
  ],
  "es6-css-minify.minifyOnSave": "exists",
  "es6-css-minify.js": {
    "mangle": true,
    "compress": {
      "sequences": true,
      "properties": true,
      "dead_code": true,
      "drop_debugger": true,
      "drop_console": true,
      "passes": 1
    },
    "output": {
      "quote_style": 0
    }
  },
  "es6-css-minify.hideButton": true
}
